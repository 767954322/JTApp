<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:slide="http://schemas.android.com/apk/res-auto"
        android:id="@+id/sliding_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        slide:umanoDragView="@+id/result_drag_view"
        slide:umanoOverlay="true"
        slide:umanoPanelHeight="@dimen/font_230"
        slide:umanoParallaxOffset="@dimen/font_100"
        slide:umanoScrollableView="@+id/result_scroll_list"
        slide:umanoShadowHeight="@dimen/font_3">

        <include layout="@layout/photo_edit_layout" />

        <include layout="@layout/result_layout" />

    </com.sothree.slidinguppanel.SlidingUpPanelLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/font_50">

        <ImageView
            android:id="@+id/result_back_button"
            android:layout_width="@dimen/size_40"
            android:layout_height="@dimen/size_40"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/size_10"
            android:background="@drawable/transparent"
            android:padding="@dimen/size_8"
            android:scaleType="centerInside"
            android:src="@drawable/tital_back"
            android:visibility="visible" />

    </RelativeLayout>


</FrameLayout>