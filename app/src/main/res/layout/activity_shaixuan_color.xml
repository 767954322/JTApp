<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main"
    android:background="@color/white">

    <include
        android:id="@+id/common_navbar"
        layout="@layout/view_navigation_header" />


    <RelativeLayout
        android:id="@+id/rl_tos_choose"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/common_navbar"
        android:background="@color/white">

        <LinearLayout
            android:id="@+id/ll_pic_choose"
            android:layout_width="match_parent"
            android:layout_height="@dimen/font_110"
            android:background="@color/white"
            android:orientation="horizontal">


            <RelativeLayout
                android:id="@+id/rl_kongjian"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/font_73"
                    android:layout_centerInParent="true"
                    android:gravity="center_horizontal">

                    <RelativeLayout
                        android:layout_width="@dimen/font_56"
                        android:layout_height="@dimen/font_56"
                        android:layout_alignParentTop="true">

                        <com.homechart.app.myview.RoundImageView
                            android:id="@+id/iv_kongjian"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/kongjian1" />
                    </RelativeLayout>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:text="空间"
                        android:textColor="@color/bg_262626"
                        android:textSize="@dimen/size_12" />

                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_jubu"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/font_73"
                    android:layout_centerInParent="true"
                    android:gravity="center_horizontal">

                    <RelativeLayout
                        android:layout_width="@dimen/font_56"
                        android:layout_height="@dimen/font_56"
                        android:layout_alignParentTop="true">

                        <com.homechart.app.myview.RoundImageView
                            android:id="@+id/iv_jubu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/jubu1" />
                    </RelativeLayout>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:text="局部"
                        android:textColor="@color/bg_262626"
                        android:textSize="@dimen/size_12" />

                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_zhuangshi"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/font_73"
                    android:layout_centerInParent="true"
                    android:gravity="center_horizontal">


                    <RelativeLayout
                        android:layout_width="@dimen/font_56"
                        android:layout_height="@dimen/font_56"
                        android:layout_alignParentTop="true">

                        <com.homechart.app.myview.RoundImageView
                            android:id="@+id/iv_zhuangshi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/zhuangshi1" />

                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:text="装饰"
                        android:textColor="@color/bg_262626"
                        android:textSize="@dimen/size_12" />

                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_shouna"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/font_73"
                    android:layout_centerInParent="true"
                    android:gravity="center_horizontal">

                    <RelativeLayout
                        android:layout_width="@dimen/font_56"
                        android:layout_height="@dimen/font_56"
                        android:layout_alignParentTop="true">

                        <com.homechart.app.myview.RoundImageView
                            android:id="@+id/iv_shouna"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/shouna1" />
                    </RelativeLayout>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:text="收纳"
                        android:textColor="@color/bg_262626"
                        android:textSize="@dimen/size_12" />

                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_secai"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_weight="1">

                <View
                    android:layout_width="@dimen/font_1"
                    android:layout_height="@dimen/font_40"
                    android:layout_centerVertical="true"
                    android:background="@color/view_line" />


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/font_73"
                    android:layout_centerInParent="true"
                    android:gravity="center_horizontal">

                    <RelativeLayout
                        android:layout_width="@dimen/font_56"
                        android:layout_height="@dimen/font_56"
                        android:layout_alignParentTop="true">

                        <com.homechart.app.myview.RoundImageView
                            android:id="@+id/iv_secai"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/secai1" />
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:text="色彩"
                        android:textColor="@color/bg_262626"
                        android:textSize="@dimen/size_12" />

                </RelativeLayout>
            </RelativeLayout>

        </LinearLayout>

        <View
            android:id="@+id/view_line_back"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/ll_pic_choose" />

        <View
            android:id="@+id/view_line_bommon"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/ll_pic_choose"
            android:background="@color/view_line" />

        <RelativeLayout
            android:id="@+id/rl_pic_change"
            android:layout_width="match_parent"
            android:layout_height="@dimen/font_44"
            android:layout_below="@+id/view_line_bommon"
            android:background="@color/white"
            android:visibility="gone">

            <ImageView
                android:id="@+id/iv_change_frag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:paddingBottom="@dimen/font_5"
                android:paddingLeft="@dimen/font_100"
                android:paddingRight="@dimen/font_12"
                android:paddingTop="@dimen/font_5"
                android:src="@drawable/pubuliu" />
        </RelativeLayout>


    </RelativeLayout>



</RelativeLayout>