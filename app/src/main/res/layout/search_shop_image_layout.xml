<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/touming">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/font_330"
        android:background="@color/bg_4d000000"
        android:orientation="vertical">

        <com.homechart.app.croplayout.EditPhotoViewMore xmlns:crop="http://schemas.android.com/apk/res-auto"
            android:id="@+id/photoedit_image_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_f7f7f7"
            crop:crop_corner_color="@color/white"
            crop:crop_line_color="@color/touming"
            crop:crop_shadow_color="@color/bg_4d000000" />
    </FrameLayout>

</RelativeLayout>